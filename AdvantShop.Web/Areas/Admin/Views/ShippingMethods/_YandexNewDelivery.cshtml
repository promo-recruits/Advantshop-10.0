@model AdvantShop.Web.Infrastructure.Admin.ShippingMethods.YandexNewDeliveryShippingAdminModel

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Токен авторизации</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.AuthorizationToken, new { @class = "form-control", ng_model = "shippingMethod.Params.AuthorizationToken", required = "required", validation_input_text = "Токен авторизации" })
        <help-trigger class="ng-cloak help-trigger-icon-abs" data-title="Токен авторизации">
            Для получения необходимо зарегистрировать приложение на Яндекс.OAuth:<br />
            1. Авторизуйтесь на Яндексе (используйте аккаунт, на который зарегистрирована компания).<br />
            2. Перейдите на страницу <a href="https://oauth.yandex.ru/client/new" target="_blank">Создание приложения</a><br />
            3. В поле Название приложения укажите название (оно может быть любым).<br />
            4. В блоке Платформы выберите Веб-сервисы. Заполните поле Callback URI: нажмите Подставить URL для разработки.<br />
            5. В поле Доступы выберите Яндекс.Доставка, отметьте опцию Использование API Яндекс.Доставки для партнеров.<br />
            6. Нажмите кнопку Создать приложение внизу страницы.
        </help-trigger>
    </div>
</div>

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Идентификатор магазина</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.ShopId, new { @class = "form-control", ng_model = "shippingMethod.Params.ShopId", required = "required", validation_input_text = "Идентификатор магазина" })
    </div>
</div>

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Идентификатор склада</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.WarehouseId, new { @class = "form-control", ng_model = "shippingMethod.Params.WarehouseId", required = "required", validation_input_text = "Идентификатор склада" })
    </div>
</div>

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Город отправителя</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.CityFrom, new { @class = "form-control", ng_model = "shippingMethod.Params.CityFrom", required = "required", validation_input_text = "Город отправителя" })
    </div>
</div>

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Отображение ПВЗ</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.DropDownListFor(x => x.TypeViewPoints, Model.ListTypesViewPoints, new { ng_model = "shippingMethod.Params.TypeViewPoints", @class = "form-control", required = "required", validation_input_text = "Отображение ПВЗ" })
    </div>
</div>

<div class="form-group row middle-xs" ng-if="shippingMethod.Params.TypeViewPoints === '1'">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">API-ключ яндекс.карт</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.YaMapsApiKey, new { @class = "form-control", ng_model = "shippingMethod.Params.YaMapsApiKey", required = "required", validation_input_text = "API-ключ яндекс.карт" })
        <help-trigger class="ng-cloak help-trigger-icon-abs" data-title="API-ключ яндекс.карт">
            <a href="https://yandex.ru/dev/maps/jsapi/doc/2.1/quick-start/index.html#get-api-key" target="_blank">Как получить ключ?</a>
        </help-trigger>
    </div>
</div>

<div class="form-group row middle-xs" ng-if="shippingMethod.Params.TypeViewPoints === '0'">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Api ключ виджета</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.WidgetApiKey, new { @class = "form-control", ng_model = "shippingMethod.Params.WidgetApiKey", required = "required", validation_input_text = "Api ключ виджета" })
        <help-trigger class="ng-cloak help-trigger-icon-abs" data-title="Api ключ виджета">
            Для получения необходимо подключить API:<br />
            1. Авторизуйтесь на Яндексе (используйте аккаунт, на который зарегистрирована компания).<br />
            2. Перейдите в <a href="https://developer.tech.yandex.ru/services/" target="_blank">Кабинет разработчика</a> и нажмите Подключить API. Выберите Виджеты Яндекс.Доставки и нажмите Продолжить.<br />
            3. Примите условия использования Яндекс.Доставки и нажмите Отправить.<br />
            4. Введите название для ключа и нажмите Получить ключ. В блоке Ключи API появится ключ.
        </help-trigger>
    </div>
</div>

<div class="form-group row middle-xs" ng-if="shippingMethod.Params.TypeViewPoints === '0'">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Название метода доставки курьером</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.CourierOptionName, new { @class = "form-control", ng_model = "shippingMethod.Params.CourierOptionName", required = "required", validation_input_text = "Название метода доставки курьером" })
    </div>
</div>
<div class="form-group row middle-xs" ng-if="shippingMethod.Params.TypeViewPoints === '0'">
