    </div>
    <div class="col-xs-6 relative">
        @Html.DropDownListFor(x => x.PriceDisplayType, Model.ListPriceDisplayType, new { ng_model = "shippingMethod.Params.PriceDisplayType", @class = "form-control", required = "required", validation_input_text = "Тип отображаемой цены доставки" })
    </div>
</div>

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">Передавать в качестве внешнего ключа заказа</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.DropDownListFor(x => x.ExportOrderExternalIdType, Model.ListExportOrderExternalIdType, new { ng_model = "shippingMethod.Params.ExportOrderExternalIdType", @class = "form-control", required = "required", validation_input_text = "Передавать в качестве внешнего ключа заказа" })
