@model AdvantShop.Web.Admin.Models.Settings.IPTelephonySettingsModel
<div class="form-group row">
    <div class="col-xs-3">
        <div class="setting-label-wrap">
            @Html.LabelFor(x => x.YandexCallbackUserKey, T("Admin.Settings.Telephony.CallBack.YandexCallbackUserKey"), new { @class = "form-label-block" })
        </div>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.YandexCallbackUserKey, new { ng_model = "settingsTelephony.yandexCallbackUserKey", @class = "form-control" })
        <help-trigger class="ng-cloak help-trigger-icon-abs" data-title="@T("Admin.Settings.Telephony.CallBack.YandexCallbackUserKey")">
            @T("Admin.Settings.Telephony.CallBack.YandexCallbackUserKeyNote")
        </help-trigger>
    </div>
</div>
<div class="form-group row">
    <div class="col-xs-3">
        <div class="setting-label-wrap">
            @Html.LabelFor(x => x.YandexCallbackBusinessNumber, T("Admin.Settings.Telephony.CallBack.YandexCallbackBusinessNumber"), new { @class = "form-label-block" })
        </div>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.YandexCallbackBusinessNumber, new { ng_model = "settingsTelephony.yandexCallbackBusinessNumber", @class = "form-control" })
        <help-trigger class="ng-cloak help-trigger-icon-abs" data-title="@T("Admin.Settings.Telephony.CallBack.YandexCallbackBusinessNumber")">
            @T("Admin.Settings.Telephony.CallBack.YandexCallbackBusinessNumberNote")
        </help-trigger>
    </div>
</div>
