@model AdvantShop.Web.Infrastructure.Admin.PaymentMethods.RbkMoney2PaymentModel

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">@T("Admin.PaymentMethods.RbkMoney2.ShopId")</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.ShopId, new { ng_model = "paymentMethod.Parameters.ShopId", @class = "form-control", required = "required", validation_input_text = T("Admin.PaymentMethods.RbkMoney2.ShopId").ToString() })
    </div>
</div>

<div class="form-group row middle-xs">
    <div class="flex-grow-n col-fixed-size-md">
        <span class="text-required">@T("Admin.PaymentMethods.RbkMoney2.ApiKey")</span>
    </div>
    <div class="col-xs-6 relative">
        @Html.TextBoxFor(x => x.ApiKey, new { ng_model = "paymentMethod.Parameters.ApiKey", @class = "form-control", required = "required", validation_input_text = T("Admin.PaymentMethods.RbkMoney2.ApiKey").ToString() })
    </div>
</div>
