        </div>
    </div>
    <div class="col-xs-5 col-wl-4" style="position: relative;">
        <div class="m-b-xs">
            <span class="text-required">@T("Admin.ExportFeed.SettingsYandex.DeliveryTimeExample")</span>
            @Html.TextBoxFor(x => Model.LocalDeliveryOption.Days, new { @class = "form-control", @maxlength = 5, ng_model = "exportFeeds.LocalDeliveryOption.Days", ng_disabled = "exportFeeds.AdvancedSettings.DeliveryCost != 'LocalDeliveryCost' || !exportFeeds.AdvancedSettings.Delivery", validation_input_text = T("Admin.ExportFeeds.YandexMarket.DeliveryTime") })
        </div>
        <div class="m-b-xs">
            @T("Admin.ExportFeeds.YandexMarket.Time")@Html.TextBoxFor(x => Model.LocalDeliveryOption.OrderBefore, new { @class = "form-control", ng_model = "exportFeeds.LocalDeliveryOption.OrderBefore", ng_disabled = "exportFeeds.AdvancedSettings.DeliveryCost != 'LocalDeliveryCost' || !exportFeeds.AdvancedSettings.Delivery" })
        </div>
        <help-trigger class="ng-cloak p-l-sm" style="position: absolute;top: 2rem;right: -1.5rem;" data-title="@T("Admin.ExportFeed.SettingsYandex.LocalDeliveryCost")">
            <div class="help-content">
                @T("Admin.ExportFeed.SettingsYandex.LocalDeliveryCostHelp")
            </div>
        </help-trigger>
    </div>
