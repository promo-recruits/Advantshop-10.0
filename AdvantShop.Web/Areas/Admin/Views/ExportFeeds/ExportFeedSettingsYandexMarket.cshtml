        </div>
    </div>
    <div class="col-xs-9 col-wl-10" ng-init="exportFeeds.setGridOptionsData(@Model.GlobalDeliveryCostJson)">
        <ui-grid-custom grid-unique-id="gridGlobalDeliveryCosts"
                        grid-on-init="exportFeeds.gridOnInit(grid)"
                        grid-filter-enabled="false"
                        grid-pagination-enabled="false"
                        grid-selection-enabled="false"
                        grid-params="{exportFeedId:@Model.ExportFeedId}"
                        grid-options="exportFeeds.gridGlobalDeliveryOptions"
                        grid-on-fetch="exportFeeds.gridOnFetch(grid)"
                        grid-extend-ctrl="exportFeeds">
        </ui-grid-custom>
        <ui-modal-trigger data-controller="'ModalAddYandexGlobalDeliveryCostCtrl'" controller-as="ctrl"
                          template-url="../areas/admin/content/src/exportFeeds/modal/addYandexGlobalDeliveryCost/addYandexGlobalDeliveryCost.html"
                          data-resolve="{'exportFeedId': @Model.ExportFeedId }"
                          data-on-close="exportFeeds.yandexAddGlobalDeliveryCost(result)">
            <button data-e2e="AddGlobalDeliveryOption" class="btn btn-sm btn-default m-t-xs" type="button" ng-disabled="exportFeeds.AdvancedSettings.DeliveryCost == 'None' || !exportFeeds.AdvancedSettings.Delivery">
                @T("Admin.ExportFeeds.YandexMarket.AddOption")
            </button>
        </ui-modal-trigger>
        <help-trigger class="ng-cloak p-l-sm" data-title="@T("Admin.ExportFeed.SettingsYandex.GlobalDeliveryCost")">
            <div class="help-content">
                @T("Admin.ExportFeed.SettingsYandex.GlobalDeliveryCostHelp")
            </div>
        </help-trigger>
    </div>
</div>

