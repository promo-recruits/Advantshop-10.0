<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="EtalonDatabaseName" value="DB_NAME" />
    <add key="TestDatabaseName" value="Test_DB_NAME" />
    <add key="BackupPath" value="C:\\DB_TAPH\bak\" />
    <add key="DatabasePath" value="C:\\DB_TAPH\" />
    <add key="SiteUrl" value="http://localhost:80" />
    <add key="SitePath" value="C:\\SITE_PATH\" />
    <add key="ScreenshotsPath" value="C:\\SCREENSHOTS_PATH\" />
  </appSettings>
  <connectionStrings>
    <add name="etalon" 
		connectionString="Data Source='MyServerName'; Connect Timeout='60'; Initial Catalog='MyDBName'; Persist Security Info='True'; User ID='MyUserName'; Password='MyPass';" 
		providerName="System.Data.SqlClient" />
		
    <add name="AdvantConnectionString" 
		connectionString="Data Source='MyServerName'; Connect Timeout='60'; Initial Catalog='MyDBName_For_Test'; Persist Security Info='True'; User ID='MyUserName'; Password='MyPass';" 
		providerName="System.Data.SqlClient" />
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-6.0.0.0" newVersion="6.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
</configuration>